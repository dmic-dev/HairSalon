<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.VBox?>

<SplitPane fx:id="genderRadio" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mic.dermitzakis.HairSalon.controller.ContactEdit.GenderRadioController">
   <items>
      <VBox prefHeight="64.0" prefWidth="183.0" spacing="11.0">
         <children>
            <Label prefHeight="27.0" prefWidth="298.0" styleClass="group-label" text="   Φύλο" />
            <RadioButton id="MALE" fx:id="maleToggle" mnemonicParsing="false" text="Άνδρας">
               <toggleGroup>
                  <ToggleGroup fx:id="genderToggleGroup" />
               </toggleGroup>
            </RadioButton>
            <RadioButton id="FEMALE" fx:id="femaleToggle" mnemonicParsing="false" selected="true" text="Γυναίκα" toggleGroup="$genderToggleGroup" />
            <RadioButton id="UNSPECIFIED" fx:id="unspecifiedToggle" mnemonicParsing="false" text="Αδιευκρίνηστο" toggleGroup="$genderToggleGroup" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="7.0" />
         </padding>
      </VBox>
   </items>
</SplitPane>
