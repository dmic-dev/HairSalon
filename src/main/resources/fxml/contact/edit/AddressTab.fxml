<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="addressTab" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mic.dermitzakis.HairSalon.controller.ContactEdit.AddressTabController">
   <children>
      <TableView fx:id="addressTable" prefHeight="85.0" prefWidth="200.0">
         <columns>
            <TableColumn fx:id="street_Column" prefWidth="212.6666578054428" text="Οδός" />
            <TableColumn fx:id="number_Column" prefWidth="84.0" text="Αριθμός" />
            <TableColumn fx:id="postCode_Column" prefWidth="93.33331298828125" text="Ταχ. Κώδικας" />
            <TableColumn fx:id="region_Column" prefWidth="175.3333740234375" text="Περιοχή" />
         </columns>
      </TableView>
      <HBox prefHeight="30.0" prefWidth="570.0" spacing="7.0">
         <children>
            <TextField fx:id="street_TextField" prefHeight="25.0" prefWidth="212.0" />
            <TextField fx:id="number_TextField" layoutX="10.0" layoutY="10.0" prefHeight="25.0" prefWidth="84.0" />
            <TextField fx:id="postCode_TextField" layoutX="159.0" layoutY="10.0" prefWidth="93.0" />
            <TextField fx:id="region_TextField" layoutX="307.0" layoutY="10.0" prefHeight="25.0" prefWidth="175.0" />
         </children>
         <padding>
            <Insets left="7.0" right="7.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER_RIGHT" prefHeight="37.0" prefWidth="570.0" spacing="10.0">
         <children>
            <Button fx:id="create_Button" mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="Δημιουργία" />
            <Button fx:id="delete_Button" mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="Διαγραφή" />
         </children>
         <padding>
            <Insets right="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
